<h1>Profiles#index</h1>
<p>Find me in app/views/profiles/index.html.erb</p>

<ul>
  <% @tombs.each do |tomb| %>
  <li>
    <%= tomb.name_tomb %>
    <%= tomb.description %>
    <%= tomb.price %>
    <% end %>
  </li>
</ul>

<ul>
  <% @bookings.each do |booking| %>
